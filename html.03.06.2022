<!--!>css<!-->


.cont
{
	width: 1170px;
	margin: 0px auto;
}

.sing
{
	color: #0099ae;
	padding:  10px 25px; 
	width: 80px;
	border-radius: 4px;
	cursor: pointer;
	text-transform: uppercase;
}

.sing:hover
{
	color: #01707f;
    border-color: #01707f;
}

.text-l
{
	align-self: center;
}

.div1
{
	flex-basis: 30%;
	text-align: center;
	transition:all 2s linear 0s; 
	
}

.div1:nth-child(1){animation: box 1s 300ms both;}
.div1:nth-child(2){animation: box 1s 600ms both;}
.div1:nth-child(3){animation: box 1s 900ms both;}

.div1:nth-child(1) img{animation: sun 0.5s 300ms both;}
.div1:nth-child(2) img{animation: sun 0.5s 600ms both;}
.div1:nth-child(3) img{animation: sun 0.5s 900ms both;}




.div1:hover
{
	transform: scale(1.2);
}

.div1 img
{
	height: 75px;
	padding: 40px 0px;		
	transition-property: height;
	transition-duration: 0.5s;
	transition: height 2s linear; 
	animation: sun 0.5s 200ms both;
}

.div1 img:hover
{
	height: 90px;
}

.slider
{
	background-image: url(img/slider-bg.png);
	background-repeat: repeat-x; 
	background-position: 0px 93%;
}

ul.nav li
{
	list-style: none;
	margin: 0px 15px;
}

.inline-b
{
display: inline-block;
}

.menu
{
 	vertical-align: top;
 	padding: 20px 0px;
 	width: 80%;
 	text-align: right;
}

ul.nav
{
	margin-block-start: 0px;
	margin-block-end: 0px;
	padding-inline-start: 0px;

}

.nav a:link{color:#000;}
.nav a:active{text-shadow: 0px 0px 10px #595959;}	
.nav a:hover{color: #00aeef;}
.nav a{text-decoration: none;}

ul.nav li:first-child:before{content: "\/";background-color: lightgreen;}

body
{
	filter:drop-shadow(0px 0px 0px blue) grayscale(0%) invert(0%) opacity(100%) saturate(100%) sepia(0%);

}

@keyframes hill
{
	0%{transform:translate(0px,-1000px);}
	100%{transform: translate(0px,0px);}
}

img .hill
{
	animation: hill 1s ease-in-out 200ms both;
}
.imag
{
	width: 40%;
	animation:birds1 500ms ease-in-out 1200ms both;
}
.text
{
	width: 50%;
	animation:birds2 500ms ease-in-out 1200ms both;
}
.koleso
{
	text-align: center;
}
.lastimg div
{
	display: inline-block;
    margin: 0px 28px;
}
.fil
{
	display: flex;
	align-items: center;
}
.sun, .house, .birds1, .birds2
{
	position: absolute;
	top: 0px;
	left: 0px;
}
.img-r
{
	position: relative;
}

.sun
{
	z-index: 1;
	top: 175px;
	left: 170px;
	animation:sun 0.5s 1s both;
}
.house
{
	top: 200px;
	left: 120px;
	animation: house 500ms ease-in-out 800ms both;
}
.birds1
{
	top: 205px;
	left: 11px;
	animation:birds1 500ms ease-in-out 1200ms both;
}
.birds2
{
	top: 155px;
	left: 500px;
	animation:birds2 500ms ease-in-out 1200ms both;
}

@keyframes sun 
{
	0%{opacity: 0; transform: scale(0);}
}
@keyframes house
{
	0%{opacity: 0;transform: translateY(-2000px);}
	60%{transform: translateY(30px);}
	80%{transform: translateY(-10px);}
}
@keyframes birds1
{
	0%{opacity: 0; transform: translateX(-20);}
}
@keyframes birds2 
{
	0%{opacity: 0; transform:translateX(20px);}
}

.fil div
{
	vertical-align: middle;
}

.right
{
	text-align: right;
	justify-content: flex-end;
}

@keyframes box 
{
	0%{opacity: 0;}
}

.box1 
{
	text-align: center;
	transition: all 0.5s linear 0s;
}
.flex_box
{
	display: flex;
}

.icons{justify-content: flex-end;flex-basis: 100%;}
.icons div {flex-basis: 2%;}
.icons i {font-size: 18px; color: #d3d3d3;cursor: pointer;}
.icons div:nth-child(2) i:hover{color: #2aa7dc;}
.icons div:nth-child(1) i:hover{color: #0884d5;}
.icons div:nth-child(3) i:hover{color: #a22523;}
.icons div:nth-child(4) i:hover{color: #e94989;}
.icons div:nth-child(5) i:hover{color: #006dc0;}

.header .flex_box
{
	flex-wrap: wrap;
	justify-content: flex-end;
}
.slider .flex_box
{
	align-items: center;
}

.free .flex_box
{
	justify-content: flex-end;
}
.koleso p{padding: 0px 31%;}
